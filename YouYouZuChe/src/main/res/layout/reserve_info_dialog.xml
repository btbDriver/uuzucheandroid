<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:layout_width="1px"
        android:layout_height="20dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:layout_marginLeft="18dp"
            android:text="预约车辆：" />

        <TextView
            android:id="@+id/brand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="预约车辆：" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:layout_marginLeft="18dp"
            android:textSize="@dimen/f5"
            android:text="取车时间：" />

        <TextView
            android:id="@+id/start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="预约车辆：" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_marginLeft="18dp"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="还车时间：" />

        <TextView
            android:id="@+id/end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="预约车辆：" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:layout_marginLeft="18dp"
            android:textSize="@dimen/f5"
            android:text="租期：" />

        <TextView
            android:id="@+id/rent_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="预约车辆：" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="18dp"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="租金：" />

        <TextView
            android:id="@+id/rent_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="预约车辆：" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="18dp"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="保险：" />

        <TextView
            android:id="@+id/safe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="预约车辆：" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_marginTop="5dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:layout_marginLeft="18dp"
            android:textSize="@dimen/f5"
            android:text="押金：" />

        <TextView
            android:id="@+id/yajin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/c8"
            android:textSize="@dimen/f5"
            android:text="预约车辆：" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="18dp"
        android:textColor="@color/c8"
        android:textSize="@dimen/f5"
        android:text="* 租金计算：满30分钟按1小时计，满8小时按1天计\n优惠券和不计免赔服务，可在预约成功后选择" />

    <View
        android:layout_width="1px"
        android:layout_height="20dp" />
</LinearLayout>